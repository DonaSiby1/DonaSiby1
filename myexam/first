import java.io.*;
import java.util.*;
public class fact
{
public static void main(String args[])
{
int fac=1;
System.out.println("Enter a number");
Scanner sc=new Scanner(System.in);
int n=sc.nextInt();
System.out.println("Entered Number is:"+n);
for(int i=1;i<=n;i++)
{
fac=fac*i;
}
System.out.println("Factorial of "+n+" is:"+fac);
}
}
